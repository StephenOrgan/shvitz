<div class="containter-fluid">
  <div class="row-fluid" id="pageHeader">
    <div class="span12" style="height:750px; background-color:white; color:black; padding-top:20px; padding-left: 20px">
            <h1>Students <%= link_to 'New Student', new_student_path, :class => 'btn btn-primary' %></h1>

            <table class="table table-striped">
              <tr>
                <th>Student Name</th>
                <th></th>
             
                <th></th>
              </tr>

            <% @students.each do |student| %>
              <tr>
                <td><%= link_to student.name, student %></td>
                <td><%= link_to 'Edit', edit_student_path(student) %></td>
                <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
            </table>

            <br />

            
</div>
  </div>
</div>